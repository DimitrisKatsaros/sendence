  "scripts": {
    "start": "npx nx run-many --target=serve --projects=sendence-backend,sendence-frontend --parallel",
    "start:fe": "npx nx serve sendence-frontend",
    "start:be": "npx nx serve sendence-backend",
    "build": "npx nx run-many --target=build --projects=sendence-backend,sendence-frontend --parallel",
    "build:fe": "npx nx build sendence-frontend",
    "build:be": "npx nx build sendence-backend",
    "test": "npx nx run-many --target=test --projects=sendence-backend,sendence-frontend --parallel",
